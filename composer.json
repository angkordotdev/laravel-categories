{
    "name": "angkor/laravel-categories",
    "description": "A fork of rinvex/laravel-categories - A polymorphic Laravel package for category management with Nested Sets, Sluggable, and Translatable support.",
    "type": "library",
    "keywords": [
        "model",
        "laravel",
        "eloquent",
        "category",
        "sluggable",
        "translatable",
        "categorizable",
        "polymorphic",
        "nested-set",
        "taxonomy",
        "angkor"
    ],
    "license": "MIT",
    "homepage": "https://open.angkor.dev",
    "support": {
        "email": "khmer@angkor.dev",
        "issues": "https://github.com/angkor/laravel-categories/issues",
        "source": "https://github.com/angkor/laravel-categories",
        "docs": "https://github.com/angkordotdev/laravel-categories/blob/main/README.md"
    },
    "authors": [
        {
            "name": "Angkor Team",
            "homepage": "https://angkor.dev",
            "email": "support@angkor.dev"
        }
    ],
    "credits": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com",
            "role": "Original Package Creator"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Original Project Lead"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "kalnoy/nestedset": "^6.0",
        "angkor/laravel-support": "^8.0",
        "spatie/laravel-sluggable": "^3.5",
        "symfony/console": "^6.4 || ^7.0"
    },
    "require-dev": {
        "illuminate/config": "^10.0 || ^11.0 || ^12.0",
        "illuminate/container": "^10.0 || ^11.0 || ^12.0",
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^12.1",
        "phpunit/php-code-coverage": "^12.1"
    },
    "autoload": {
        "psr-4": {
            "Angkor\\Categories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Angkor\\Categories\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Angkor\\Categories\\Providers\\CategoriesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
